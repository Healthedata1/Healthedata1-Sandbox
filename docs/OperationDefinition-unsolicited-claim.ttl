  fhir:DomainResource.text [
     fhir:Narrative.status [ fhir:value "extensions" ];
     fhir:Narrative.div "<div xmlns=\"http://www.w3.org/1999/xhtml\"><h2>Unsolicited Claims Attachment Operation</h2><p>OPERATION: Unsolicited Claims Attachment Operation</p><p>The official URL for this operation definition is: </p><pre>http://hl7.org/fhir/us/davinci-cdex/OperationDefinition/unsolicited-claim</pre><div><p>This operation is used to submit an unsolicited claims attachment as a single claim (or simply a reference to a claim) and other referenced resources as attachments for processing. The input parameters are:</p>\n<ol>\n<li>the X12 Attachment Control Number</li>\n<li>the claim either as:\n<ol>\n<li>ASC X12N 278 (Health Care Services Review — Notification and Acknowledgment) and an ASC X12N 275 (Additional Information to Support a Health Care Services Review) or</li>\n<li>Claim resource.</li>\n</ol>\n</li>\n<li>Attachments as FHIR Resources to support the claims review.<br/>\nThere are no output parameters.</li>\n</ol>\n</div><p>URL: [base]/Claim/$unsolicited-claim</p><p>Parameters</p><table class=\"grid\"><tr><td><b>Use</b></td><td><b>Name</b></td><td><b>Cardinality</b></td><td><b>Type</b></td><td><b>Binding</b></td><td><b>Documentation</b></td></tr><tr><td>IN</td><td>AttachmentControlNumber</td><td>1..1</td><td><a href=\"http://hl7.org/fhir/R4/datatypes.html#string\">string</a></td><td/><td><div><p>The Attachment Control Number is assigned by the provider which will also be reported in the X12N 275 submission.</p>\n</div></td></tr><tr><td>IN</td><td>X12Claim</td><td>0..1</td><td><a href=\"http://hl7.org/fhir/R4/documentreference.html\">DocumentReference</a></td><td/><td><div><p>An X12 ASC X12N 278 (Health Care Services Review — Notification and Acknowledgment) and an ASC X12N 275 (Additional Information to Support a Health Care Services Review) binary file or url.</p>\n</div></td></tr><tr><td>IN</td><td>FHIRClaim</td><td>0..1</td><td><a href=\"http://hl7.org/fhir/R4/claim.html\">Claim</a></td><td/><td><div><p>A FHIR Claim resource.</p>\n</div></td></tr><tr><td>IN</td><td>Attachment</td><td>0..*</td><td><a href=\"http://hl7.org/fhir/R4/resource.html\">Resource</a></td><td/><td><div><p>Attachments as FHIR Resources to support the claims review.</p>\n</div></td></tr></table></div>"
  ];
  fhir:OperationDefinition.url [ fhir:value "http://hl7.org/fhir/us/davinci-cdex/OperationDefinition/unsolicited-claim"];
  fhir:OperationDefinition.version [ fhir:value "0.1.0"];
  fhir:OperationDefinition.name [ fhir:value "Unsolicited Claims Attachment Operation"];
  fhir:OperationDefinition.status [ fhir:value "draft"];
